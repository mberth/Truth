{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2017",
		"inlineSourceMap": true,
		"inlineSources": true,
		"stripInternal": true,
		"strict": true,
		"rootDir": "./",
		"baseUrl": "./",
		"outDir": "./Build/Dev",
		"declaration": false,
		"lib": [
			"es7",
			"es2017",
			"es2017.object"
		]
	},
	"include": [
		"**/*.ts"
	],
	"exclude": [
		"./Build/**/*.ts",
		"./Release/**/*.ts",
		"./CoreAgents/**/*.ts",
		"./TypesBundler/**/*.ts"
	]
}
